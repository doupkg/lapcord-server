{
  "name": "@doupkg/lapcord",
  "version": "2.1.0",
  "description": "Module required to run Lapcord.",
  "packageManager": "yarn@3.5.0",
  "bin": "lib/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "author": {
    "name": "Dou",
    "url": "https://github.com/doupkg"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doupkg/lapcord-server.git"
  },
  "scripts": {
    "test": "mocha",
    "build": "swc src -d lib --copy-files",
    "lint": "rome check src",
    "lint:fix": "rome check --apply src",
    "format": "rome format src --semicolons as-needed --quote-style single --trailing-comma none",
    "format:fix": "rome format --write src --semicolons as-needed --quote-style single --trailing-comma none",
    "clean:lock": "node scripts/removeLock.cjs"
  },
  "devDependencies": {
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.40",
    "@swc/register": "0.1.10",
    "@types/node": "18.15.11",
    "@xhayper/discord-rpc": "1.0.15",
    "commander": "10.0.1",
    "mocha": "10.2.0",
    "rome": "12.0.0",
    "typescript": "5.0.4",
    "vscode-languageserver": "8.1.0",
    "vscode-languageserver-textdocument": "1.0.8"
  },
  "license": "MIT"
}
